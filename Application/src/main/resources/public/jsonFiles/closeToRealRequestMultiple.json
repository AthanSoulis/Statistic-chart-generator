{
    "library" : "Highcharts",

    "chartDescription": 
    {
        "chart": {
            "type": "line"
        },
        "title": {
            "text": "Publications per Year"
        },        
        "yAxis": {
            "title": {
            "text": "# of Publications"
            }
        },
        "series": [{
            "name": "H2020",
            "query": {
                "filters": [
                    {
                        "field": "project.publication.year",
                        "type": "between",
                        "values": ["2010", "2015"]
                    },
                    {	"field": "project.funding level 0",
                        "type": "=",
                        "values": ["H2020"]
                    }
                ],
                "select": [
                    {
                        "field": "project.publication.year"
                    },
                    {
                      "aggregate": "count",
                      "field": "project"
                    }
                ],
                "entity": "project"
            }

        },
        {
            "name": "FP7",
            "query":{
            "filters": [
                {
                    "field": "project.publication.year",
                    "type": "between",
                    "values": ["2010", "2015"]
                },
                {	"field": "project.funding level 0",
                    "type": "=",
                    "values": ["FP7"]
                }
            ],
            "select": [
                {
                    "field": "project.publication.year"
                },
                {
                    "aggregate": "count",
                    "field": "project"
                }
            ],
            "entity": "project"
            }
        }]        
    }
}