{
  "library": "HighCharts",
  "chartDescription": {
    "chart": {
      "type": "bar"
    },
    "title": {
      "text": "Publications per Year from H2020"
    },
    "yAxis": {
      "title": {
        "text": "# of Publications"
      }
    },
    "queries": [
      {
        "query": {
          "filters": [
            {
              "field": "project.publication.year",
              "type": "between",
              "values": [
                "2010",
                "2015"
              ]
            },
            {
              "field": "project.funding level 0",
              "type": "=",
              "values": [
                "H2020"
              ]
            }
          ],
          "select": [
            {
              "aggregate": "count",
              "field": "project"
            },
            {
              "field": "project.publication.year"
            }
          ],
          "entity": "project"
        }
      }
    ]
  }
}