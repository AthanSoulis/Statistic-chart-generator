{
    "library" : "Highcharts",

    "chartDescription": 
    {
        "chart": {
            "type": "bar"
        },
        "title": {
            "text": "Publications per Year from H2020"
        },        
        "yAxis": {
            "title": {
            "text": "# of Publications"
            }
        },
        "series": [{
            "name": "H2020",
            "query": {
                "filters": [
                    {
                        "field": "project.publication.year",
                        "type": "between",
                        "value1": 2010,
                        "value2": 2015
                    },
                    {	"field": "project.funding level 0",
                        "type": "equal",
                        "value1": "H2020"
                    }
                ],
                "select": [
                    {
                        "field": "project.publication.year"
                    },
                    {
                      "aggregate": "count",
                      "field": "project"
                    }
                ],
                "entity": "project"
            }
        }]               
    }
}