{
    "library":"GoogleCharts",

    "chartDescription":
    {   
        "chartType":"BarChart",
        "columns": ["Year","H2020"],
        "queries":[
            {"query": {
                "filters": [
                    {
                        "field": "project.publication.year",
                        "type": "between",
                        "value1": 2010,
                        "value2": 2015
                    },
                    {	"field": "project.funding level 0",
                        "type": "equal",
                        "value1": "H2020"
                    }
                ],
                "select": [
                    {
                        "field": "project.publication.year"
                    },
                    {
                      "aggregate": "count",
                      "field": "project"
                    }
                ],
                "entity": "project"
            }}
        ],
        "options":{
            "title" : "Publications per Year from H2020",
            "hAxis": {"title": "# of Publications"}  
        }
    }
}