{
  "library": "GoogleCharts",
  "chartDescription": {
    "chartType": "ComboChart",
    "columns": [
      "Dates",
      "H2020",
      "FP7"
    ],
    "queries": [
      {
        "query": {
          "filters": [
            {
              "field": "project.publication.date",
              "type": "between",
              "values": [
                "2017-01-01",
                "2017-02-01"
              ]
            },
            {
              "field": "project.funding level 0",
              "type": "=",
              "values": [
                "FP7"
              ]
            }
          ],
          "select": [
            {
              "field": "project.publication.date"
            },
            {
              "aggregate": "count",
              "field": "project"
            }
          ],
          "entity": "project"
        }
      },
      {
        "query": {
          "filters": [
            {
              "field": "project.publication.date",
              "type": "between",
              "values": [
                "2017-01-01",
                "2017-01-15"
              ]
            },
            {
              "field": "project.funding level 0",
              "type": "=",
              "values": [
                "FP7"
              ]
            }
          ],
          "select": [
            {
              "field": "project.publication.date"
            },
            {
              "aggregate": "count",
              "field": "project"
            }
          ],
          "entity": "project"
        }
      }
    ],
    "options": {
      "title": "Publications per Day",
      "vAxis": {
        "title": "# of Publications"
      },
      "seriesType": "bars",
      "series": [
        {
          "type": "bars"
        },
        {
          "type": "area"
        }
      ]
    }
  }
}